{
  "name": "laravel-log-viewer-table",
  "displayName": "Laravel Log Viewer Table",
  "description": "View your Laravel Log files in a easy-to-read table, with filters and pagination.",
  "author": "Nathan James <nathan@nathanjms.co.uk>",
  "publisher": "nathanjames",
  "homepage": "https://github.com/Nathanjms/laravel-log-viewer-extension#readme",
  "bugs": {
    "url": "https://github.com/Nathanjms/laravel-log-viewer-extension/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nathanjms/laravel-log-viewer-extension.git"
  },
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "laravel-log-viewer.formatLogs",
        "title": "Laravel Log Viewer: View as Table"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
